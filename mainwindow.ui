	color: white;	
	background: black;
	selection-color: green;
	selection-background-color: gray;
}

QTabWidget
{
}

QTabWidget::pane {
		border-color: rgb(0,0,0);
		border-style: solid;
		border-width: 0px;
		border-radius: 6px;
}

QTabBar::tab {	
	color:gray;
	font: 14pt ;
	min-width: 100px;
	min-height: 30px;
	padding: 5px;
	border-width: 1px;
	border-style: solid;
	border-bottom-color: black; 
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	margin-right:5px;
	margin-left:5px;
 
}

QTabWidget::tab-bar{
    alignment: center;
}

QTabBar::tab:selected
{
    background-color: black;
    color:rgb(255,255,255);
    font: 14pt;
}

QGroupBox {
    background-color: white;
    border: 1px solid black;
    border-radius: 5px;
    margin-top: 1px;
 	padding: 10px;
}

QGroupBox::title {
	color:gray;
    padding: 0px 0px 0px 0px;
	margin:0px 0px 0px 0px;
    background-color:white;
}

QScrollArea {
  	 border-color: gray;
     border-style: solid;
	 border-width:0px;
 	 border-radius: 0px;
}

QScrollBar:vertical {
     border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 0px;
	 color:red;
     background: white;
 	 margin: 25px 0px 25px 0px;
}
QScrollBar::handle:vertical {
	border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 3px;
	margin: 1px 2px 1px 2px;
    background: gray;
    min-width: 20px;
}

QScrollBar::add-line:vertical {
   border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
   border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar:up-arrow:vertical, QScrollBar::down-arrow:vertical {
	border: 2px solid gray;
    width: 5px;
    height: 5px;
    background: white;
}

QScrollBar:horizontal {
    border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 0px;
	 color:red;
     background: white;
    height: 20px;
    margin: 0px 25px 0px 25px;
}
QScrollBar::handle:horizontal {
   border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 3px;
	margin: 1px 2px 1px 2px;
    background: gray;
    min-width: 20px;
}
QScrollBar::add-line:horizontal {
  border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
  border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
   border: 2px solid gray;
    width: 5px;
    height: 5px;
    background: white;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

