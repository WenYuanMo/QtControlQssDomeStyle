}

QLineEdit {
alignment:center;
	border-width: 1px; 
	border-radius: 5px;
	border-color: rgb(0, 0, 0);
	border-style: inset;
	padding: 5px;
	color: black;
	background:white;
	selection-background-color:black;
	selection-color: white;
	font:14px;
}

QLabel {
alignment:center;
	border-width: 0px; 
	border-radius: 5px;
	border-color: rgb(0, 0, 0);
	border-style: inset;
	padding: 5px;
	color: rgb(0,0,0);
	background:white;
}

QSpinBox {
	border-width: 2px; 
	border-radius:5px;
	color: white;	
	background-color: black;
	padding: 5;
}

QDoubleSpinBox {
	border-width: 2px; 
	border-radius: 5px;
	color: white;	
	background-color: black;
	padding: 5;
}

QProgressBar {
	text-align: center;
	color: rgb(0, 0, 0);
	border-width: 2px; 
	border-radius: 10px;
	border-color: rgb(0, 0, 0);
	border-style: inset;
	background-color:white;
	padding:5px;
}

QProgressBar::chunk {
	background-color: gray;
	border-radius: 10px;
}

QPushButton:hover
{
    color:#888888;
    background-color:rgb(0, 0, 0); 
    border-style:inset;
    padding-left:8px;
    padding-top:8px;
}

QPushButton:pressed
{
    color:#ffffff;
    background-color:rgb(0, 0, 0); 
    border-style:inset;
    padding-left:6px;
    padding-top:6px;
}

QPushButton
{
    color:black; 
    background-color:rgb(255,255, 255);
    border-style:outset; 
    border-width:2px;
    border-color:rgb(0, 0,0);
    border-radius:10px;
    font: 14px;
    min-width:100px;
    min-height:20px;
    padding:5px;
}

QCheckBox {
	color: rgb(0,0,0);
	padding: 2px;
}

QCheckBox:disabled {
	color: red;
	padding: 2px;
}

QCheckBox::hover {
	border-radius:4px;
	border-style:solid;
	padding-left: 1px;
	padding-right: 1px;
	padding-bottom: 1px;
	padding-top: 1px;
	border-width:0px;    
	border-color: rgb(0, 0, 0);
	background-color:#000000;
	color: rgb(255,255,255);
}

QCheckBox::indicator:checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: gray;
	background-color: gray;
}
QCheckBox::indicator:unchecked {

	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: gray;
	background-color: white;
}


QRadioButton {
	color: black;
	background-color: rgb(255,255,255);
	padding: 1px;
}
QRadioButton::indicator:checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius:5px;
	border-width: 1px;
	border-color: gray;
	background-color: gray;
}
QRadioButton::indicator:!checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius:5px;
	border-width: 1px;
	border-color: gray;
	background-color: transparent;
}

QComboBox {
	color: black;	
	background: white;
	border-style:solid;
	border-width:2px;
	border-color:black;
	border-radius:5px;
	padding: 1px;
}

QComboBox:editable {
	background: black;
	color: white;
}

QComboBox:!editable {
	background: black;
	color: white;
}

